{
    "name": "alaxos/cakephp3-libs",
    "description": "Custom libraries for CakePHP framework (>= v3.4)",
    "type": "cakephp-plugin",
    "keywords" : ["cakephp", "libraries"],
    "homepage": "https://github.com/alaxos/cakephp3-libs",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "~3.4",
        "cakephp/plugin-installer": "*"
    },
    "require-dev": {
		"phpunit/phpunit": "^5.7|^6.0"
	},
    "autoload": {
        "psr-4": {
            "Alaxos\\": "src",
            "Alaxos\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Alaxos\\Test\\": "tests"
        }
    },
    "extra": {
        "installer-name": "Alaxos"
    },
    "support": {
    	"source": "https://github.com/alaxos/cakephp3-libs"
    }
}

