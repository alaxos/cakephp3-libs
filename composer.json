{
    "name": "alaxos/cakephp3-libs",
    "description": "Custom libraries for CakePHP 3.x framework",
    "type": "cakephp-plugin",
    "keywords" : ["cakephp", "libraries"],
    "version": "0.1.0",
    "homepage": "https://github.com/alaxos/cakephp3-libs",
    "license": "MIT",
    "require": {
        "php": ">=5.4.19",
        "composer/installers": "*",
        "cakephp/cakephp": ">=3.0"
    },
    "autoload": {
        "psr-4": {
            "Alaxos\\": "src",
            "Alaxos\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Alaxos\\Test\\": "tests"
        }
    },
    "extra": {
        "installer-name": "Alaxos"
    }
}